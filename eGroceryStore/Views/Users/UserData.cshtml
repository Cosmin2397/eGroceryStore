@using Microsoft.AspNetCore.Identity
@using eGroceryStore.Areas.Data
@model eGroceryStore.Areas.Data.ApplicationUser

<div class = "main-wrapper">
        <div class = "container">
            <div class = "product-div">
                <div class = "product-div-right">
                    <span class = "product-price">First Name:@Model.FirstName</span>
                    <span class = "product-price">Last Name: @Model.LastName</span>
                    <span class = "product-price">E-mail: @Model.Email</span>
                    <span class = "product-price">Id: @Model.Id</span>
                    <span class = "product-price">Phone Number: @Model.PhoneNumber</span>
                </div>
            </div>
        </div>
    </div>